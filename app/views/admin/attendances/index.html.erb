<header>
	<h1>Attendances</h1>
	</header>
	
<table class="table table-striped table-responsive">
  <thead>
    <tr>
      <th>Thinker</th>
      <th>Event</th>
    </tr>
  </thead>

  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.thinker %></td>
        <td><%= link_to attendance.attended_thought.title, thought_path(attendance.attended_thought) %></td>
    <% end %>
  </tbody>

</table>


